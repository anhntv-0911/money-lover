<%= form_for [:admin, @saving] do |f| %>
  <%= render "shared/error_messages", object: f.object %>

  <%= f.label :saving_in_year %>
  <%= f.number_field :saving_in_year, class: "form-control" %>

  <%= f.submit yield(:button_text), class: "btn btn-primary" %>
  <%= link_to t("button.back"), :back, class: "btn btn-default" %>
<% end %>
